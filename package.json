    "autoprefixer": "^9.8.6",
    "css-loader": "^4.2.1",
    "lerna": "^3.22.1",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^9.0.3",
